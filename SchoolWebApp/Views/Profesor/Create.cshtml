@model SchoolTracker.Model.Profesor

@{
    ViewData["Title"] = "Dodaj novog profesora";
}

<nav aria-label="breadcrumb">
	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a asp-action="Index">Popis profesora</a></li>
		<li class="breadcrumb-item active" aria-current="page">Novi profesor</li>
